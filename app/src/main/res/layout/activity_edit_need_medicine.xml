<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue"
    tools:context=".EditNeedMedicine">

    <TextView
        android:id="@+id/unstextView6"
        android:layout_width="376dp"
        android:layout_height="58dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="@string/EditneedMedicine"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/unstextView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.695" />

    <TextView
        android:id="@+id/usnstextView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="68dp"
        android:text="@string/moto_addAvailable"
        android:textColor="?attr/colorPrimary"
        android:textSize="20sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/unsBottumNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.242"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.201" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/unsmedicinename"
        style="@style/LoginTextInputOuterFieldStyle"
        android:layout_width="360dp"
        android:layout_height="75dp"
        android:layout_marginHorizontal="6dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        app:boxStrokeColor="@color/purple_700"
        app:hintTextColor="@color/purple_700"
        app:layout_constraintBottom_toTopOf="@+id/unsmWeightLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unstextView6">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/unsmname"
            style="@style/LoginTextInputInnerFieldStyle"
            android:layout_width="355dp"
            android:layout_height="70dp"
            android:ems="10"
            android:hint="@string/avFeed_enter_medicineName"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/unsmWeightLayout"
        style="@style/LoginTextInputOuterFieldStyle"
        android:layout_width="360dp"
        android:layout_height="75dp"
        android:layout_marginHorizontal="6dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="33dp"
        app:boxStrokeColor="@color/purple_700"
        app:hintTextColor="@color/purple_700"
        app:layout_constraintBottom_toTopOf="@+id/unsmedicinequantity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unsmedicinename">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/unsmWeight"
            style="@style/LoginTextInputInnerFieldStyle"
            android:layout_width="355dp"
            android:layout_height="75dp"
            android:hint="@string/avFeed_enter_medicineWeight"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/unsmedicinequantity"
        style="@style/LoginTextInputOuterFieldStyle"
        android:layout_width="360dp"
        android:layout_height="75dp"
        android:layout_marginHorizontal="6dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="100dp"
        app:boxStrokeColor="@color/purple_700"
        app:hintTextColor="@color/purple_700"
        app:layout_constraintBottom_toTopOf="@+id/BottumNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unsmedicinename">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/unsquantity"
            style="@style/LoginTextInputInnerFieldStyle"
            android:layout_width="355dp"
            android:layout_height="70dp"
            android:hint="@string/avFeed_enter_medicineQty"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/unsmedicinedescription"
        style="@style/LoginTextInputOuterFieldStyle"
        android:layout_width="360dp"
        android:layout_height="75dp"
        android:layout_marginHorizontal="6dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="80dp"
        app:boxStrokeColor="@color/purple_700"
        app:hintTextColor="@color/purple_700"
        app:layout_constraintBottom_toTopOf="@+id/unsconstraintLayout3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unsmedicinequantity">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/unsdescription2"
            style="@style/LoginTextInputInnerFieldStyle"
            android:layout_width="355dp"
            android:layout_height="70dp"
            android:layout_weight="1"
            android:hint="@string/avFeed_enter_medicineDescription"
            android:inputType="text" />
    </com.google.android.material.textfield.TextInputLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/unsconstraintLayout3"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/to_background"
        app:layout_constraintEnd_toStartOf="@+id/unsconstraintLayout4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/unsmedicinedescription"
        app:layout_constraintVertical_bias="0.888">

        <TextView
            android:id="@+id/unsbuttons"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/addAvailable_btn_submit"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/unsconstraintLayout4"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/to_background"
        app:layout_constraintBottom_toTopOf="@+id/BottumNavigationView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/unsconstraintLayout3"
        app:layout_constraintTop_toBottomOf="@+id/unsmedicinedescription"
        app:layout_constraintVertical_bias="0.0">

        <TextView
            android:id="@+id/unsbuttonC"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/addAvailable_btn_cancel"
            android:textColor="@color/black"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/BottumNavigationView"
        android:layout_width="411dp"
        android:layout_height="60dp"
        android:background="@color/bottom_nav_background"
        app:itemIconTint="@drawable/bottom_nav_colors"
        app:itemTextColor="@drawable/bottom_nav_colors"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottum_nav_menu" />

</androidx.constraintlayout.widget.ConstraintLayout>